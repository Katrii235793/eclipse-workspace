package ustdemo;

import java.util.Scanner;

public class patt3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scan=new Scanner(System.in);
		int i=0;
		int a[];
		int sum=0,avg;
		a=new int [10];
		System.out.println("Start Entering");
		for(i=0;i<10;i++)
		{
		a[i]=scan.nextInt();
		}
		for(i=0;i<10;i++)
		{
	 sum=sum+a[i];
		}

	avg=sum/10;
	System.out.println(avg+" ");
	//System.out.println();
}

	}




