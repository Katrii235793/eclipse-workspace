package ustdemo;

import java.util.Scanner;

public class patt3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scan=new Scanner(System.in);
		int i = 0;
		int a[];
		a=new int [10];
		System.out.println("Start Entering");
		for(i=1;i<=10;i++)
		a[i]=scan.nextInt();
for(i=1;i<=10;i++)
{
	int sum=0,avg;
	sum=sum+a[i];
	avg=sum/10;
	System.out.print((avg)+" ");
	//System.out.println();
}
	}

}
