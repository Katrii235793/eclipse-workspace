package ustdemo;

public class stardes {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n=5,j;
		for(int i = 0;i<=n;i++)
		{
			for( j=0;j<=n-i;j--)
			{
				System.out.print("*");
			}
			//for( j=0;j<=i;j++)
			{
				System.out.println(" ");
			}
			//System.out.println();
		}
			}

		}

	}

}
