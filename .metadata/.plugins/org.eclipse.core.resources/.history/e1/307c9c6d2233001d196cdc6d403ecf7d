package sorting;

public class selection {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		 int arr[] = {23,43,13,65,11,62,76,83,9,71,84,34,96,80};
	       
	        int n = arr.length;
	      
	        for(int i=0;i<n-1;i++) {
	            for (int j = 0; j<n-1; j++) {
	            	int MIN=i;
	                
	            	if(arr[j]<arr[MIN]) {
	                	int temp=arr[i];
	                	arr[i]=arr[MIN];
	                	arr[MIN]=temp;
	                }
	            }
	        }
	    
	       
	        for (int j = 0; j<n-1; j++) {
	     
	        System.out.println(arr[j]);
	    }


	   }
	}

	


